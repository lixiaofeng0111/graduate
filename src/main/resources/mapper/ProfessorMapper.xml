  <select id = "selectProfessorsByTopicKindId" resultMap="professor">
  	select * from graduate_professor as
		a
		WHERE 6 > (
		select count(*)
		from graduate_professor
		where topicKind_id = a.topicKind_id
		and id > a.id)
		order by a.id desc;
  </select>
  
